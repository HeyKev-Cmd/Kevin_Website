{
    "builds": [
        {
        "src": "kevin_web/wsgi.py",
        "use": "@vercel/vercel-python-wsg",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
        }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "kevin_web/wsgi.py"
        }
    ]
}